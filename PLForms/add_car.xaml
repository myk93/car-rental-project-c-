        <TextBox x:Name="tb_City" HorizontalAlignment="Left" Height="23" Margin="352,95,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="107" TextChanged="tb_City_TextChanged"/>
        <TextBox x:Name="tb_str" HorizontalAlignment="Left" Height="23" Margin="224,95,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="88" RenderTransformOrigin="0.908,0.609"/>
