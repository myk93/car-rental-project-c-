        <Label Content="סוג הרישיון" HorizontalAlignment="Left" Margin="389,192,0,0" VerticalAlignment="Top" Width="70" Height="26"/>
        <ComboBox x:Name="cobo" HorizontalAlignment="Left" Margin="389,219,0,0" VerticalAlignment="Top" Width="70" RenderTransformOrigin="0.5,0.5">
            <ComboBoxItem Content="A" HorizontalAlignment="Center" />
            <ComboBoxItem Content="B" HorizontalAlignment="Center" />
            <ComboBoxItem Content="C" HorizontalAlignment="Center" />
            <ComboBoxItem Content="D" HorizontalAlignment="Center" />
            <ComboBoxItem Content="E" HorizontalAlignment="Center" />
        </ComboBox>
        <Label Content="אין חובה למלות שדה זה*" HorizontalAlignment="Left" Margin="30,400,0,0" VerticalAlignment="Top" Width="141"/>
        <DatePicker Name="d_ya" HorizontalAlignment="Left" Margin="110,41,0,0" VerticalAlignment="Top"/>
        <DatePicker Name="d_rr" HorizontalAlignment="Left" Margin="220,219,0,0" VerticalAlignment="Top" Width="113"/>

