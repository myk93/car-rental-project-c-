        <Label Content="רחוב" HorizontalAlignment="Left" Margin="240,251,0,0" VerticalAlignment="Top" Width="70" Height="26"/>
        <Label Content="בנין" HorizontalAlignment="Left" Margin="157,252,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="tb_City_Copy" HorizontalAlignment="Left" Height="23" Margin="352,277,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="104" TextChanged="tb_City_Copy_TextChanged"/>
        <TextBox x:Name="tb_str_Copy" HorizontalAlignment="Left" Height="23" Margin="232,277,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="78" RenderTransformOrigin="0.908,0.609" TextChanged="tb_str_Copy_TextChanged"/>
        <ComboBox x:Name="me_bo_Copy" HorizontalAlignment="Left" Margin="144,278,0,0" VerticalAlignment="Top" Width="51" RenderTransformOrigin="0.075,0.727"/>
