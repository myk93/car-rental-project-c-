        <ComboBox Name="cb_nos" HorizontalAlignment="Left" Margin="235,156,0,0" VerticalAlignment="Top" Width="50"/>
        <Label Content="מספר דלתות" HorizontalAlignment="Left" Margin="136,152,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.526,1.308"/>
        <ComboBox Name="cb_door" HorizontalAlignment="Left" Margin="76,156,0,0" VerticalAlignment="Top" Width="42"/>
