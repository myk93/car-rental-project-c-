        <Button Content="Button" HorizontalAlignment="Left" Margin="247,374,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click"/>
        <Label Content="מספר רכב" HorizontalAlignment="Left" Margin="480,38,0,0" VerticalAlignment="Top" Width="63"/>
        <TextBox Name="tb_nur" HorizontalAlignment="Left" Height="23" Margin="355,41,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120" TextChanged="TextBox_TextChanged"/>
        <Label Content="*תאריך יצור" HorizontalAlignment="Left" Margin="232,38,0,0" VerticalAlignment="Top"/>
        <Label Content="פרטי הרכב" HorizontalAlignment="Left" Margin="477,88,0,0" VerticalAlignment="Top" Width="79" Height="26"/>
        <Label Content="יצרן" HorizontalAlignment="Left" Margin="369,69,0,0" VerticalAlignment="Top" Width="79" Height="26"/>
        <Label Content="סוג" HorizontalAlignment="Left" Margin="244,69,0,0" VerticalAlignment="Top" Width="78" Height="26"/>
